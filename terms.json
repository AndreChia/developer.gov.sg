---
layout: null
---
[
    {% for term in site.data.terms %}
        {
            "term": "{{ term.term }}",
            "full_term": "{{ term.full_term }}",
            "description": "{{ term.description }}",
            "link": "{{ term.link }}",
            "category": [
                {% for category in term.category %}
                "{{ category }}"{% unless forloop.last %},{% endunless %}
                {% endfor %}
            ]
        }{% unless forloop.last %},{% endunless %}
    {% endfor %}
]
    "scripts": {
        "build:lambda": "netlify-lambda build lambda",
        "build:site": "npm run build:static && bundle exec jekyll build",
        "build:static": "NODE_ENV=production webpack",
        "deploy": "npm run test:lambda && npm run build:lambda && npm run test:site && npm run build:site",
        "dev:jekyll": "$HOME/.rbenv/shims/bundle exec jekyll serve",
        "dev:lambda": "netlify-lambda serve lambda",
        "dev:netlify": "NODE_ENV=development netlify dev",
        "dev:server": "npm run dev:static & npm run dev:jekyll",
        "dev:static": "webpack --watch",
        "test": "echo \"Error: no test specified\" && exit 1",
        "test:lambda": "NODE_ENV=production mocha --recursive './lambda/test/*.js'",
        "test:site": "exit 0"
    },
